{
	"name": "pythagus/laravel-logger",
	"description": "Package to format and send Laravel logs to a website",
	"license": "MIT",
	"type": "package",
	"authors": [
		{
			"name": "Damien Molina",
			"email": "opensource@dmolina.fr"
		}
	],
	"require": {
		"php": "^7.3|^8.0",
		"anlutro/curl": "^1.5",
		"monolog/monolog": "^2.0",
		"laravel/framework": "^6.0 | ^7.0 | ^8.0 | ^9.0"
	},
	"autoload": {
		"psr-4": {
			"Pythagus\\LaravelLogger\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Pythagus\\LaravelLogger\\LoggerServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
